{
  "name": "account",
  "version": "1.0.0",
  "description": "Online store product account service",
  "scripts": {
    "start": "db-migrate up && node ./build/account.server.js",
    "dev": "cross-env TS_NODE_FILES=true nodemon --watch src --exec 'ts-node' src/account.server.ts",
    "dev-with-lb": "concurrently 'env PORT=3001 npm run dev' 'env PORT=3002 npm run dev' 'env PORT=3003 npm run dev' --prefix account-service-{index}-{pid} --prefix-colors red,green,yellow,blue,magenta,cyan,white,gray",
    "build": "npm run compile",
    "compile": "tsc"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "concurrently": "^6.5.0",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.24",
    "@types/passport-auth0": "^1.0.2",
    "chai": "^4.3.4",
    "cross-env": "^5.2.1",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "mocha": "^9.1.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  }
}
