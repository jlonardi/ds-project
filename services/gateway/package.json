{
  "name": "gateway",
  "version": "1.0.0",
  "description": "Online store api gateway",
  "main": "index.js",
  "scripts": {
    "start": "db-migrate up && node ./build/gateway/src/gateway.server.js",
    "dev": "TS_NODE_FILES=true nodemon --watch src --exec 'ts-node' src/gateway.server.ts",
    "build": "npm run compile && npm run static && npm run views",
    "static": "cp -r ./src/ui/static/ ./build/static/",
    "views": "cp -r ./src/ui/views/ ./build/views/",
    "compile": "tsc",
    "test": "mocha -r ts-node/register tests/**/*.test.ts"
  },
  "homepage": "https://github.com/jlonardi/node-auth0-template#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "connect-pg-simple": "^7.0.0",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.13",
    "passport": "^0.5.0",
    "passport-auth0": "^1.4.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^12.12.24",
    "@types/passport-auth0": "^1.0.2",
    "chai": "^4.3.4",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "mocha": "^9.1.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  }
}
